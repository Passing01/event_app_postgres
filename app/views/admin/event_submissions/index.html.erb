<h1>Admin::EventSubmissions#index</h1>
<p>Find me in app/views/admin/event_submissions/index.html.erb</p>
<h1>Événements à valider</h1>
<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td>
          <%= link_to 'Voir', [:admin, event] %>
          <%= link_to 'Modifier', edit_admin_event_submission_path(event) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
